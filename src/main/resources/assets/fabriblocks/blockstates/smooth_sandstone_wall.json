{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "fabriblocks:block/smooth_sandstone_wall_post" }
        },
        {   "when": { "OR": [ { "north": "low" }, { "north": "tall" } ] },
            "apply": { "model": "fabriblocks:block/smooth_sandstone_wall_side", "uvlock": true }
        },
        {   "when": { "OR": [ { "east": "low" }, { "east": "tall" } ] },
            "apply": { "model": "fabriblocks:block/smooth_sandstone_wall_side", "y": 90, "uvlock": true }
        },
        {   "when": { "OR": [ { "south": "low" }, { "south": "tall" } ] },
            "apply": { "model": "fabriblocks:block/smooth_sandstone_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "OR": [ { "west": "low" }, { "west": "tall" } ] },
            "apply": { "model": "fabriblocks:block/smooth_sandstone_wall_side", "y": 270, "uvlock": true }
        }
    ]
}
